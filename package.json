{
  "name": "wordpress-rag-assistant",
  "version": "1.0.0",
  "description": "AI-powered WordPress plugin troubleshooting system using RAG (Retrieval-Augmented Generation)",
  "main": "./server/server.ts",
  "type": "module",
  "scripts": {
    "start": "tsx server/server.ts",
    "dev": "tsx watch server/server.ts",
    "build": "tsc",
    "test": "tsx test-server.ts",
    "frontend": "webpack-dev-server --hot --progress --color",
    "build-frontend": "webpack --mode production",
    "dev-all": "concurrently \"npm run dev\" \"npm run frontend\""
  },
  "keywords": [
    "wordpress", 
    "troubleshooting", 
    "rag", 
    "llm", 
    "openai",
    "plugins",
    "ai-assistant"
  ],
  "author": "Your Name",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.8.6",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "babel-loader": "^9.2.1",
    "concurrently": "^8.2.2",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.2",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "style-loader": "^4.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.6.3",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.1",
    "fs": "^0.0.1-security",
    "openai": "^4.98.0",
    "path": "^0.12.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
